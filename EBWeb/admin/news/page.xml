    <taskSetItem name="updateNews">
      <script1>eio.appendString8(ee.getText(ee.getControl("panel9")));
eio.appendString16(ee.getEditText(ee.getControl("edit1")));
eio.appendString16(ee.getEditText(ee.getControl("edit2")));
eio.appendString16(ee.getEditText(ee.getControl("richedit1")));
</script1>
      <script2>var newsId = eio.readString8();
var title  = eio.readString16();
var subTitle  = eio.readString16();
var data  = eio.readString16();
var sql = "update news set title = '" + db.encode(title) + "'" +
	",subTitle='" + db.encode(subTitle) + "'" +
	",data='" + db.encode(data) + "'" +
	" where id = " + newsId;
if (db.executeNoneQuery("EBWeb",sql)) {
	eio.appendString8("T");
} else {
	eio.appendString8("F");
}</script2>
