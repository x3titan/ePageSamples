      <onPageLoad><![CDATA[g5.init = function(data) {
    g5.params = params = eval("(" + data + ")");
}

g5.getParam = function(key) {
    for (var i = 0; i < g5.params.length; i++) {
        if (g5.params[i].length != 2) continue;
        if (g5.params[i][0].toLowerCase() == key.toLowerCase()) return g5.params[i][1];
    }
    return "";
}

g5.log = function(text) {
    text = ee.getText(ee.getControl("panel3")) + "\r\n" + text.trim();
    ee.setText(ee.getControl("panel3"), text);
}

g5.post = function(url, params) {
    ee.setEditText(ee.getControl("edit2"), url);
    ee.setEditText(ee.getControl("edit1"), params);
    g5.ajax = new TamAjax("g5.ajax");
    g5.ajax.createHandle();
    g5.ajax.onReceive = function(name) {  
        var result = g5.ajax.responseText; 
        g5.log("<b>接收:</b>" + result);
        g5.init(result);
        g5.onPost();    
    }
    g5.ajax.post(url, params);
    g5.log("<font color=#ffff00><b>发送:</b>" + url + " -> " + params + "</font>");
}        


]]></onPageLoad>
