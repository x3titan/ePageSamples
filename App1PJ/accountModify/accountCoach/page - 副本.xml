      <script2><![CDATA[var token = eio.readString8();
var sql,sqlResult;
sql = "exec getUserInfoBySession '" + db.encode(token) + "'";
sqlResult = db.executeQuery("PJ", sql);
if (sqlResult == null) {
    eio.appendString8("F");
    es.logWarning("数据库执行错误, sql=" + sql);
    return;
}
db.read(sqlResult);
if (db.getString(sqlResult, "sessionId").length < 5) {
    eio.appendString8("F");
    eio.appendString8("token失效");
    es.logWarning("用户token失效, sql=" + sql);
    return;
} 
var userId = db.getString(sqlResult, "id");       

sql="select * from userInfo where id="+userId;
sqlResult = db.executeQuery("PJ", sql); 
db.read(sqlResult);     
var autoStatus = db.getString(sqlResult, "autoStatus");  
es.logWarning(autoStatus);
var photo2 = db.getString(sqlResult, "photo2");   
var photo3 = db.getString(sqlResult, "photo3"); 
var photo5 = db.getString(sqlResult, "photo5"); 
var photo6 = db.getString(sqlResult, "photo6"); 
var photo7 = db.getString(sqlResult, "photo7");   
if(autoStatus=="VVN"||autoStatus=="VVV"){
    eio.appendString8("F"); 
    eio.appendString16("已认证，无需再次申请");  
     return;
} 
if(photo3!=""&amp;&amp;photo5!=""&amp;&amp;photo6!=""&amp;&amp;photo7!=""){
    sql="update userInfo set autoStatus='VCN'where id="+userId;
    sqlResult = db.executeQuery("PJ", sql);
    db.read(sqlResult);
    eio.appendString8("T");
    es.logWarning(sql);
}  
