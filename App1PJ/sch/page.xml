      <onControlLoad><![CDATA[var isIOS = !!navigator.userAgent.match(/\(i[^;]+;( U;)? CPU.+Mac OS X/);
if (!isIOS) {
    ee.getControl("layoutyitem1").heavy = 0;
}

g5.pjSch =
    "FFFFFFFFFFFFFFFFFFFFFFFF" +
    "FFFFFFFFFFFFFFFFFFFFFFFF" +
    "FFFFFFFFFFFFFFFFFFFFFFFF" +
    "FFFFFFFFFFFFFFFFFFFFFFFF" +
    "FFFFFFFFFFFFFFFFFFFFFFFF" +
    "FFFFFFFFFFFFFFFFFFFFFFFF" +
    "FFFFFFFFFFFFFFFFFFFFFFFF";
g5.pjSchCurrent = 0;
g5.pjSchButton = new Array();
g5.pjSchButton.push(ee.getControl("panel3"));
g5.pjSchButton.push(ee.getControl("panel4"));
g5.pjSchButton.push(ee.getControl("panel5"));
g5.pjSchButton.push(ee.getControl("panel6"));
g5.pjSchButton.push(ee.getControl("panel7"));
g5.pjSchButton.push(ee.getControl("panel8"));
g5.pjSchButton.push(ee.getControl("panel10"));

g5.refresh = function() {
    //刷新顶部按钮
    var i;
    for (i = 0; i < g5.pjSchButton.length; i++) {                       
        if (g5.pjSchCurrent == i) {
            ee.setSkin(g5.pjSchButton[i], "/App1PJ/skins/button3.png",27,23,11,9,3,3);
            ee.setTextColor(g5.pjSchButton[i], "#ffffff");
        } else {
            ee.setSkin(g5.pjSchButton[i], "/App1PJ/skins/line3.png",37,35,7,7,23,21);
            ee.setTextColor(g5.pjSchButton[i], "#000000");
        }
    }
    //表格内容
    var ch;
    var cell;
    for (i = 0; i < 15; i++) {
        ch = g5.pjSch.charAt(g5.pjSchCurrent*24+i+7);
        cell = ee.getTableCell(ee.getControl("tablebody1"), i%3, parseInt(i/3));
        cell = cell.eeChildren[0]; 
        if (ch=="T") {
            ee.setSkin(cell, "/App1PJ/skins/button3.png",27,23,11,9,3,3);
            ee.setTextColor(cell, "#ffffff");
        } else {    
            ee.setSkin(cell, "/App1PJ/skins/line3.png",37,35,7,7,23,21);
            ee.setTextColor(cell, "#000000"); //3a83bf
        } 
        ee.setText(cell, (i + 7 < 10 ? "0" + (i + 7) : i + 7) + ":00"); 
        cell.parentNode.onclick = function () {
            var pos = this.rowIndex * 3 + this.colIndex + 7 + g5.pjSchCurrent * 24;
            if (g5.pjSch.charAt(pos) == "T") {
                g5.pjSch = g5.pjSch.slice(0, pos) + "F" + g5.pjSch.slice(pos + 1);
            } else {
                g5.pjSch = g5.pjSch.slice(0, pos) + "T" + g5.pjSch.slice(pos + 1);
            }
            g5.refresh();
        }
    }
    
}
g5.refresh();
refresh();

]]></onControlLoad>
