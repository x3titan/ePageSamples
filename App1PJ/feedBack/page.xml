    <taskSetItem name="feedBack">
      <script1><![CDATA[var a = ee.getEditText(ee.getControl("edit1"));
if ((a.length<6)||(a.length>200)) {
    alert("对不起，问题和建议的字数必须在6-200字范围内。");
    return;
}
eio.appendString8(eb.getCookie("pjToken"));
eio.appendString16(a);
]]></script1>
      <script2><![CDATA[var token = eio.readString8();
var sql,sqlResult;
sql = "exec getUserInfoBySession '" + db.encode(token) + "'";
sqlResult = db.executeQuery("PJ", sql);
if (sqlResult == null) {
    eio.appendString8("F");
    es.logWarning("数据库执行错误, sql=" + sql);
    return;
}
db.read(sqlResult);
if (db.getString(sqlResult, "sessionId").length < 5) {
    eio.appendString8("F");
    eio.appendString8("token失效");
    es.logWarning("用户token失效, sql=" + sql);
    return;
} 
var userId = db.getString(sqlResult, "id");

var text = eio.readString16();
sql = 
    "insert into feedBack (userId,text) values (" +
    userId + ",'" + db.encode(text) + "')";
if (!db.executeNoneQuery("PJ", sql)) {
    eio.appendString8("F");
    eio.appendString8("系统繁忙，无法获取账户信息");
    return;
}    
sql = 
    "insert into sendNews (userId,title,newsContent,status) values (" +
    userId + ",'意见反馈提交通知','您的反馈已经提交，非常感谢您对1陪驾的关心和支持，我们的后台工作人员会在1-3个工作日内对您的意见和建议进行处理。谢谢！','未读')";
db.executeNoneQuery("PJ", sql)
eio.appendString8("T");

]]></script2>
