      <script1>//今天生日榜</script1>
      <script2>var sqlString = 
	"select r.ryxx_jobnum, " +
	"r.ryxx_birth, " +
 	"s.zzgs_name || ' ' || b.zzbm_name || ' ' || x.gwxx_postname || ' ' || " +
	"r.ryxx_name as rname, " +
	"s.zzgs_parent_id, " +
	"r.ryxx_tel, " +
	"r.ryxx_mobile, " +
	"r.ryxx_email " +
	"  from y_ryxx r " +
	"  left join y_zzbm b on r.zzbm_id = b.zzbm_id " +
	"  left join y_zzgs s on r.zzgs_id = s.zzgs_id " +
	"  left join y_gwxx x on r.gwxx_id = x.gwxx_id " +
	" where r.ryxx_birth like '%' || to_char(sysdate - 0, 'mm-dd') ";
var sqlResult = db.executeQuery("ZAOracle", sqlString);
eio.appendDataSet(sqlResult, "rname", 0, 100);

</script2>
