var e3 = eio.readString16();
var e4 = eio.readString16();
var e5 = eio.readString16();
var e6 = eio.readString16();
var e7 = eio.readString16();
var e8 = eio.readString16();
var e9 = eio.readString16();
var sql;
sql = "select * from manufacturerInfo where accountName = '" + db.encode(e1) + "'";
var sqlResult = db.executeQuery("QR", sql);
if (!db.read(sqlResult)) {
    sql = "insert into manufacturerInfo (accountName, accountPassword, cnName, email, website, postAddress, contactName, contactTel, note) values (" +
        "'" + db.encode(e1) + "'," +
        "'" + db.encode(es.getMd5(e2)) + "'," +
        "'" + db.encode(e3) + "'," +
        "'" + db.encode(e4) + "'," +
        "'" + db.encode(e5) + "'," +
        "'" + db.encode(e6) + "'," +
        "'" + db.encode(e7) + "'," +
        "'" + db.encode(e8) + "'," +
        "'" + db.encode(e9) + "') ";
    if (!db.executeNoneQuery("QR", sql)) {
        eio.appendString8("系统繁忙，请稍后再试。");
    } else {
        eio.appendString8("T");
    }
} else {
    eio.appendString8("用户名" + e1 + "已存在，请重新选择一个用户名");
}







</script2>
