var e8 = ee.getEditText(ee.getControl("edit8"));
var e9 = ee.getEditText(ee.getControl("edit9"));
var e10 = ee.getEditText(ee.getControl("edit10"));
//客户端验证
if (e1.length==0) {
    alert("用户名属于必填项，请认真填写");
    return;
}
if (e2.length<6) {
    alert("密码长度必须大于等于6位");
    return;
}
if (e2!=e3) {
    alert("对不起，您两次输入的密码不一致");
    return;
}
//发送
eio.appendString16(e1);
eio.appendString16(e2);
eio.appendString16(e4);
eio.appendString16(e5);
eio.appendString16(e6);
eio.appendString16(e7);
eio.appendString16(e8);
eio.appendString16(e9);
eio.appendString16(e10);





]]></script1>
      <script2>var e1 = eio.readString16();
