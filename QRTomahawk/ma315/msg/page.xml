      <script1>eio.appendInt32(g5.msgId);</script1>
      <script2>var msgId = eio.readInt32();
//获取newsType
var sql, sqlResult;
sql = "select * from news where id = " + msgId;
sqlResult = db.executeQuery("EBWeb", sql);
db.read(sqlResult);
var newsType = db.getString(sqlResult, "newsType");
//同类消息列表
sql = "select title + '`' + cast(id as varchar(20)) as title from news where siteId = 2 and newsType = '" + db.encode(newsType) + "' order by regdatetime desc";
sqlResult = db.executeQuery("EBWeb", sql);
eio.appendDataSet(sqlResult, "title", 0, 20);
</script2>
