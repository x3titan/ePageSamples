    <taskSetItem name="getPage">
      <script1>eio.appendInt32(g5.msgId);</script1>
      <script2>var msgId = eio.readInt32();
var sql, sqlResult;
sql = "select title, subTitle, data, convert(varchar, regdatetime, 120) as regdatetime from news where id = " + msgId;
sqlResult = db.executeQuery("EBWeb", sql);
db.read(sqlResult);
eio.appendString8(db.getString(sqlResult,"title"));
eio.appendString8(db.getString(sqlResult,"subTitle"));
eio.appendString8(db.getString(sqlResult,"regdatetime").substring(0, 16));
eio.appendString32(db.getString(sqlResult,"data"));







</script2>
