      <onControlLoad><![CDATA[//滚动式菜单
g5.gmenuControl = new Array();
g5.aniEnabled = false;
g5.gmenuControl.push(ee.getControl("layoutyitem4")); //主菜单
g5.gmenuControl.push(ee.getControl("layoutyitem6"));
g5.gmenuControl.push(ee.getControl("layoutyitem8"));
g5.gmenuControl.push(ee.getControl("layoutyitem10"));
g5.gmenuControl.push(ee.getControl("layoutyitem12"));
g5.gmenuControl[0].subMenu = ee.getControl("layoutyitem5"); //子菜单
g5.gmenuControl[1].subMenu = ee.getControl("layoutyitem7");
g5.gmenuControl[2].subMenu = ee.getControl("layoutyitem9");
g5.gmenuControl[3].subMenu = ee.getControl("layoutyitem11");
g5.gmenuControl[4].subMenu = ee.getControl("layoutyitem13");
g5.gmenuControl[0].subMenuHeight = 230; //子菜单高度
g5.gmenuControl[1].subMenuHeight = 230;
g5.gmenuControl[2].subMenuHeight = 230;
g5.gmenuControl[3].subMenuHeight = 230;
g5.gmenuControl[4].subMenuHeight = 230;

g5.gmenuInit = function () {
    var i;
    for (i = 0; i < g5.gmenuControl.length; i++) {
        ee.setRect(g5.gmenuControl[i].subMenu,null,null,null,1);
        g5.gmenuControl[i].open = false;
        g5.gmenuControl[i].aniEnabled = false;
        g5.gmenuControl[i].aniStartH = 0;
        g5.gmenuControl[i].aniEndH = 0;
    }
}
g5.gmenuClick = function (index) {
    if ((index<0)||(index>=g5.gmenuControl.length)) return;
    if (g5.gmenuControl[index].open) { 
        //ee.setRect(g5.gmenuControl[index].subMenu,null,null,null,1);
        g5.gmenuStartAni(index, 1);
    } else {
        //ee.setRect(g5.gmenuControl[index].subMenu,null,null,null,g5.gmenuControl[index].subMenuHeight); 
        g5.gmenuStartAni(index, g5.gmenuControl[index].subMenuHeight);
    }
    g5.gmenuControl[index].open = !g5.gmenuControl[index].open;
    ee.setRect(g5.gmenuControl[index].parentNode, null, null, null, null, true);
}
g5.gmenuStartAni = function (index, endH) {
    g5.gmenuControl[index].aniEnabled = true;
    g5.gmenuControl[index].aniStartH = eb.getDivRect(g5.gmenuControl[index].subMenu).h;
    g5.gmenuControl[index].aniEndH = endH;
    g5.gmenuControl[index].aniStartTime = new Date();
    //判断动画是否已经开始
    if (!g5.aniEnabled) {
        g5.aniEnabled = true;
        setTimeout(g5.gmenuTimer, 1);
    }
}
g5.gmenuTimer = function () {
    var i;
    //动画处理
    for (i = 0; i < g5.gmenuControl.length; i++) {
        if (!g5.gmenuControl[i].aniEnabled) continue;
        var time = (new Date()).getTime() - g5.gmenuControl[i].aniStartTime.getTime();
        time = time / 600;
        time = Math.min(Math.max(0, time), 1);
        var pos = eb.getInterpolation("bonus", time);
        pos = parseInt(pos*(g5.gmenuControl[i].aniEndH - g5.gmenuControl[i].aniStartH)+g5.gmenuControl[i].aniStartH);
        ee.setRect(g5.gmenuControl[i].subMenu,null,null,null, pos);
        //动画是否结束
        if (time >= 1) {
            g5.gmenuControl[i].aniEnabled = false;
        }   
    }
    ee.setRect(g5.gmenuControl[0].parentNode, null, null, null, null, true);

    //判断是否所有的动画都结束了
    for (i = 0; i < g5.gmenuControl.length; i++) {
        if (g5.gmenuControl[i].aniEnabled) {
            setTimeout(g5.gmenuTimer, 1);
        }
    }
    g5.aniEnabled = false;    
}
g5.gmenuInit();
                            
//frame
g5.frame = document.createElement("iframe");
g5.frame.style.position = "absolute";
g5.frame.style.width = "100%";
g5.frame.style.height = "100%";
g5.frame.style.scrolling = "auto";
g5.frame.style.overflow = "auto";
g5.frame.setAttribute("frameborder", "0", 0);
ee.getControl("layoutxitem2").appendChild(g5.frame);

 ]]></onControlLoad>
